  xsi:schemaLocation="http://www.pchapin.org/XML/AOML AOML.xsd">

  <datetimerange>
    <start resolution="PT10M">
      <ISO>2003-01-01T00:00:00Z</ISO>
    </start>
    <end resolution="PT10M">
      <ISO>2003-01-01T00:10:00Z</ISO>
    </end>
  </datetimerange>
  <location>
    <value>
      <lat-long-alt>+43.00-072.50+300</lat-long-alt>
    </value>
    <notes>
      <xhtml:p>US-Vermont-Tunbridge</xhtml:p>
    </notes>
  </location>
  <observer>Peter C. Chapin</observer>
  <equipment>
    <notes>
      <xhtml:p>75mm Unitron refractor...</xhtml:p>
    </notes>
  </equipment>
  <notes>
    <xhtml:p>My plan for the evening was...</xhtml:p>
  </notes>

